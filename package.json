{
  "name": "@wppconnect/frontend",
  "version": "0.1.0",
  "description": "Projeto feito para autenticar a automomacao do WhatsappWeb com multi-clientes de forma dinamica. Backend feito em Nodejs(express, socketio), FrontEnd (ReactJS)",
  "main": "index.js",
  "author": "kingaspx",
  "license": "Apache-2.0",
  "scripts": {
    "build": "react-scripts build",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "cz",
    "eject": "react-scripts eject",
    "license:add": "license-check-and-add add",
    "license:check": "license-check-and-add check",
    "prepare": "husky install",
    "release": "release-it",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.24",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "axios": "^0.21.1",
    "emoji-regex": "^9.2.2",
    "lucide-react": "^0.15.11",
    "mic-recorder-to-mp3": "^2.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-json-csv": "^2.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "serialize-javascript": "^5.0.1",
    "socket.io-client": "^4.0.0",
    "styled-components": "^5.2.1",
    "sweetalert2": "^11.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "license-check-and-add": "^4.0.2",
    "prop-types": "^15.7.2",
    "release-it": "^14.8.0"
  }
}
